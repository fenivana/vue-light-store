  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/vue-light-store/issues"
  },
  "homepage": "https://github.com/fenivana/vue-light-store#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.1.0"
  },
  "babel": {
    "presets": [
