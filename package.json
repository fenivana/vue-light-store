{
  "name": "vue-light-store",
  "version": "0.2.4",
  "description": "Simple Vue store with support of modules",
  "main": "dist/Store.umd.js",
  "module": "dist/Store.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/vue-light-store.git"
  },
  "keywords": [
    "vue",
    "store"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/vue-light-store/issues"
  },
  "homepage": "https://github.com/fenivana/vue-light-store#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-enough": "^0.3.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough"
  }
}
